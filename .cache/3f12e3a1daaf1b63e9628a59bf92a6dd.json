{"id":"component11.html","dependencies":[],"generated":{"html":"<!DOCTYPE html>\r\n<html lang=\"en\">\r\n<head>\r\n    <meta charset=\"UTF-8\">\r\n    <meta http-equiv=\"X-UA-Compatible\" content=\"IE=edge\">\r\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\r\n    <title>动态组件</title>\r\n</head>\r\n<body>\r\n    <div id=\"app\">\r\n        <component :is=\"thisView\"></component>\r\n        <button @click=\"handleView('A')\">第一句</button>\r\n        <button @click=\"handleView('B')\">第二句</button>    \r\n        <button @click=\"handleView('C')\">第三句</button>    \r\n\r\n    </div>\r\n\r\n\r\n\r\n    <script src=\"https://cdn.jsdelivr.net/npm/vue@2/dist/vue.js\"></script>\r\n    <script>\r\n        //通过点击不同的按钮切换不同的视图\r\n        Vue.component('compA',{\r\n            template:'<div>我是视图A</div>'\r\n        })\r\n        Vue.component('compB',{\r\n            template:'<div>我是视图B</div>'\r\n        })\r\n        Vue.component('compC',{\r\n            template:'<div>我是视图C</div>'\r\n        })  \r\n        var app=new Vue({\r\n            el:\"#app\",\r\n            data:{\r\n                thisView:'compA'\r\n            },\r\n            methods:{\r\n                handleView:function(tag){\r\n                    this.thisView='comp'+tag;\r\n                }\r\n            }\r\n        })\r\n    </script>\r\n</body>\r\n</html>"},"hash":"eeff2d86af055cdea4d5d55595d30548","cacheData":{}}