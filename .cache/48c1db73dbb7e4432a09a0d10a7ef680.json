{"id":"render4.html","dependencies":[],"generated":{"html":"<!DOCTYPE html>\r\n<html lang=\"en\">\r\n<head>\r\n    <meta charset=\"UTF-8\">\r\n    <meta http-equiv=\"X-UA-Compatible\" content=\"IE=edge\">\r\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\r\n    <title>在render函数中使用props传递</title>\r\n</head>\r\n<body>\r\n    <div id=\"app\">\r\n        <button @click=\"switchShow\">点击切换照片</button>\r\n        <my-component :show=\"show\"></my-component>\r\n    </div>\r\n\r\n\r\n\r\n    <script src=\"https://cdn.jsdelivr.net/npm/vue@2/dist/vue.js\"></script>\r\n    <script>\r\n        //点击按钮切换图片\r\n        Vue.component('my-component',{\r\n            props:['show'],\r\n            render:function(createElement){\r\n                if(this.show){\r\n                    var imgsrc;\r\n                    if(this.show){\r\n                        imgsrc='src/imge/001.jpg'\r\n                    }\r\n                    else{\r\n                        imgsrc='src/imge/002.jpg'\r\n\r\n                    }\r\n                    return createElement('imge',{\r\n                        attrs:{\r\n                            src:imgsrc\r\n                        }\r\n                    })\r\n                }\r\n            }\r\n\r\n        })\r\n        var app=new Vue({\r\n            el:\"#app\",\r\n            data:{\r\n                show:false\r\n            },\r\n            methods:{\r\n                switchShow:function(){\r\n                    this.show=!this.show\r\n                }\r\n            }\r\n        })\r\n    </script>\r\n</body>\r\n</html>"},"hash":"422164650b8ff46c766bd322a16bf9d9","cacheData":{}}